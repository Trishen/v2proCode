@model IEnumerable<v2proCode.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>List Of Students Applications</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StudentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentGender)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentTown)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentContact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentGrade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentBirthCertURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentReportURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentProofResURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentPermitURL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StudentAllowReg)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StudentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentSurname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentGender)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentTown)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentContact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentGrade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentBirthCertURL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentReportURL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentProofResURL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentPermitURL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudentAllowReg)
        </td>
        <td>
            @Html.ActionLink("Accept", "Edit", new { id=item.StID }) |
            @Html.ActionLink("Decline", "Delete", new { id=item.StID })
        </td>
    </tr>
}

</table>
